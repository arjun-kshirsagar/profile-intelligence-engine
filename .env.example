DATABASE_URL=postgresql+psycopg://postgres:postgres@localhost:5432/profile_engine
REQUEST_TIMEOUT_SECONDS=8
GOOGLE_CSE_API_KEY=your_google_cse_api_key
GOOGLE_CSE_CX=your_google_cse_cx

# Optional reflective layer
LLM_REFLECTION_ENABLED=false

# OpenAI configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4.1-mini

# Groq configuration
GROQ_API_KEY=your_groq_api_key
GROQ_MODEL=llama-3.3-70b-versatile

# Gemini configuration (script generation agent)
GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-3-flash
SCRIPT_GENERATION_MAX_ATTEMPTS=4
